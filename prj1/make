#include <boost/lexical_cast.hpp>
#include <iostream>
#include <stdio.h>
#include <vector>

using namespace std;

class example
{
public:
    void print();
protect:
private:
};

void example::print()
{
    printf("example print\n");
}

int main(int argc, char* argv[])
{
    using boost::lexical_cast;
    int a = lexical_cast<int>("123");
    double b = lexical_cast<double>("123.12");
    std::cout<<a<<std::endl;
    std::cout<<b<<std::endl;

    cout << "main" << endl;
    example e;
    e.print();


	return 0;
}
